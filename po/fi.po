msgstr "Tuodaan kirjoja..."

#: ui/Importer.qml:142
msgid "Waiting"
msgstr "Odottaa"

#: ui/Importer.qml:144
msgid "Processing"
msgstr "Käsittelee"

#: ui/Importer.qml:146
msgid "Imported to %1"
msgstr "Tuotu kohteeseen %1"

#: ui/Importer.qml:148
msgid "Already in library: %1"
msgstr "Entuudestaan kirjastossa: %1"

#: ui/Importer.qml:150
msgid "Error: %1"
msgstr "Virhe: %1"

#: ui/Importer.qml:171
msgid "Import books from"
msgstr "Kirjojen tuonnin lähde"

#: ui/BookPage.qml:101
msgid "Pages read today: %1"
msgstr "Luettuja sivuja tänään: %1"

#: ui/BookPage.qml:102
msgid "No pages read today"
msgstr "Ei tänään luettuja sivuja"

#: ui/BookPage.qml:178
msgid "Page"
msgstr "Sivu"

#: ui/BookPage.qml:201
msgid "Contents"
msgstr "Sisältö"

#: ui/BookPage.qml:422
msgid "Default Font"
msgstr "Oletusfontti"

#. / Prefer string of < 16 characters /
#: ui/BookPage.qml:434
msgid "Font Scaling"
msgstr "Fontin skaalaus"

