msgstr "Xa existe na biblioteca: 1%"

#: ui/Importer.qml:150
msgid "Error: %1"
msgstr "Erro: %1"

#: ui/Importer.qml:171
msgid "Import books from"
msgstr "Importar libros de"

#: ui/BookPage.qml:101
msgid "Pages read today: %1"
msgstr "Páxinas lidas hoxe: 1%"

#: ui/BookPage.qml:102
msgid "No pages read today"
msgstr "Hoxe non leu ningunha páxina"

#: ui/BookPage.qml:178
msgid "Page"
msgstr "Páxina"

#: ui/BookPage.qml:201
msgid "Contents"
msgstr "Contidos"

#: ui/BookPage.qml:422
msgid "Default Font"
msgstr "Letra predeterminada"

