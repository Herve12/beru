msgstr "Oczekiwanie"

#: ui/Importer.qml:144
msgid "Processing"
msgstr "Przetwarzanie"

#: ui/Importer.qml:146
msgid "Imported to %1"
msgstr "Zaimportowano do %1"

#: ui/Importer.qml:148
msgid "Already in library: %1"
msgstr "Zawartość biblioteki: %1"

#: ui/Importer.qml:150
msgid "Error: %1"
msgstr "Błąd: %1"

#: ui/Importer.qml:171
msgid "Import books from"
msgstr "Importuj książki z"

#: ui/BookPage.qml:101
msgid "Pages read today: %1"
msgstr "Dzis przeczytano %1 stron"

#: ui/BookPage.qml:102
msgid "No pages read today"
msgstr "Nic dzisiaj nie przeczytano"

#: ui/BookPage.qml:178
msgid "Page"
msgstr "Strona"

#: ui/BookPage.qml:201
msgid "Contents"
msgstr "Treść"

#: ui/BookPage.qml:422
msgid "Default Font"
msgstr "Domyślna czcionka"

